# Configuration de la base de données PostgreSQL
DB_NAME=votre_nom_de_base_de_donnees
DB_USER=votre_utilisateur_postgres
DB_PASSWORD=votre_mot_de_passe
DB_HOST=localhost
DB_PORT=5432

# Clé secrète Django (générez une nouvelle clé pour la production)
SECRET_KEY=votre_cle_secrete_django

# Mode debug (True pour le développement, False pour la production)
DEBUG=True

# Hosts autorisés (séparés par des virgules)
ALLOWED_HOSTS=localhost,127.0.0.1